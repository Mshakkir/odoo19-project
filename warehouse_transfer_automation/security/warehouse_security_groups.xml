<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- FYH Warehouse Group -->
        <record id="group_fyh_warehouse" model="res.groups">
            <field name="name">FYH Warehouse User</field>
            <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
        </record>

        <!-- DMM Warehouse Group -->
        <record id="group_dmm_warehouse" model="res.groups">
            <field name="name">DMM Warehouse User</field>
            <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
        </record>

        <!-- BLD Warehouse Group -->
        <record id="group_bld_warehouse" model="res.groups">
            <field name="name">BLD Warehouse User</field>
            <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
        </record>
    </data>
</odoo>














