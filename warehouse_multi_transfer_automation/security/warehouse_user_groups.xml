<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Security Groups for SSAOCO Warehouses -->

        <!-- Main Warehouse Group -->
        <record id="group_warehouse_ssaoco_main" model="res.groups">
            <field name="name">SSAOCO-Main User</field>
            <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
        </record>

        <!-- Baladiya Warehouse Group -->
        <record id="group_warehouse_ssaoco_baladiya" model="res.groups">
            <field name="name">SSAOCO-Baladiya User</field>
            <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
        </record>

        <!-- Dammam Warehouse Group -->
        <record id="group_warehouse_ssaoco_dammam" model="res.groups">
            <field name="name">SSAOCO-Dammam User</field>
            <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
        </record>

    </data>
</odoo>