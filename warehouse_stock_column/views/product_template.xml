<odoo>
    <record id="product_template_tree_view_warehouse_qty" model="ir.ui.view">
        <field name="name">product.template.tree.warehouse.qty</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_tree_view"/>
        <field name="priority">100</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='standard_price']" position="after">
                <field name="qty_wh_stock" string="WH/Stocked" optional="show"/>
                <field name="qty_dw_stock" string="DW/Stock" optional="show"/>
                <field name="qty_balad_stock" string="Balad/Stock" optional="show"/>
            </xpath>
        </field>
    </record>
</odoo>