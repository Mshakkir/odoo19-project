<odoo>
    <record id="view_account_list_balance_sheet" model="ir.ui.view">
        <field name="name">custom.balance.sheet.line.list</field>
        <field name="model">custom.balance.sheet.line</field>
        <field name="arch" type="xml">
            <list string="Balance Sheet Details">
                <field name="account_type"/>
                <field name="name"/>
                <field name="debit"/>
                <field name="credit"/>
                <field name="balance"/>
            </list>
        </field>
    </record>
</odoo>
