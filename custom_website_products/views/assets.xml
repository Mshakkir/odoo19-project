<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <template id="assets_frontend" inherit_id="web.assets_frontend" name="Custom Products Assets Frontend">
            <xpath expr="//link[contains(@href, 'web.assets_frontend')]" position="after">
                <link rel="stylesheet" href="/custom_website_products/static/css/custom_products.css"/>
            </xpath>
        </template>

        <template id="assets_common" inherit_id="web.assets_common" name="Custom Products Assets Common">
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/custom_website_products/static/js/custom_products.js"/>
            </xpath>
        </template>
    </data>
</odoo>