<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Override the preview template with a unique ID -->
    <template id="custom_quote_preview_override" inherit_id="sale.quote_document_layout_preview" priority="100">

        <!-- Find and replace the t-call to report_saleorder_document -->
        <xpath expr="//t[@t-call='sale.report_saleorder_document']" position="replace">
            <t t-call="custom_sa_quotation.custom_saleorder_report">
                <t t-set="docs" t-value="[doc]"/>
            </t>
        </xpath>

    </template>
</odoo>