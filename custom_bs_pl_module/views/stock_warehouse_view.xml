<!-- views/stock_warehouse_view.xml -->
<odoo>
    <record id="view_warehouse_form_inherit_analytic" model="ir.ui.view">
        <field name="name">stock.warehouse.form.inherit.analytic</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group" position="inside">
                <field name="analytic_account_id"/>
            </xpath>
        </field>
    </record>
</odoo>
