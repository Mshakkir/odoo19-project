<odoo>
    <record id="product_template_tree_view_warehouse_qty" model="ir.ui.view">
        <field name="name">product.template.tree.warehouse.qty</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_tree_view"/>
        <field name="priority">100</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='standard_price']" position="after">
                <field name="qty_fyh_stock" string="FYH/Stock" optional="show"/>
                <field name="qty_bld_stock" string="BLD/Stock" optional="show"/>
                <field name="qty_dmm_stock" string="DMM-1/Stock" optional="show"/>
            </xpath>
        </field>
    </record>
</odoo>