<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Sequence for Bank Statement -->
    <record id="seq_bank_statement" model="ir.sequence">
        <field name="name">Bank Statement Reconciliation</field>
        <field name="code">bank.statement</field>
        <field name="prefix">BRS/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>

