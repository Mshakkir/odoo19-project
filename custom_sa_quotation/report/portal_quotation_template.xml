<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Override the entire preview template -->
    <template id="custom_quote_preview_override" inherit_id="sale.quote_document_layout_preview" priority="100">

        <!-- Replace the entire template content -->
        <xpath expr="//t[@t-name='sale.quote_document_layout_preview']" position="replace">
            <t t-name="sale.quote_document_layout_preview">
                <t t-call="web.html_preview_container">
                    <t t-call="custom_sa_quotation.custom_saleorder_report"/>
                </t>
            </t>
        </xpath>

    </template>
</odoo>