<odoo>
    <record id="view_daybook_analytic_wizard_form" model="ir.ui.view">
        <field name="name">daybook.analytic.wizard.form</field>
        <field name="model">daybook.analytic.wizard</field>
        <field name="arch" type="xml">
            <form string="Daybook Analytic Report">
                <group>
                    <field name="date_from"/>
                    <field name="date_to"/>
                    <field name="analytic_account_id"/>
                </group>
                <footer>
                    <button name="action_show_details" string="Show Details" type="object" class="btn-secondary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
